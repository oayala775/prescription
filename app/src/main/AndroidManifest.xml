<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prescription"
        tools:targetApi="31">

        <!-- Es la principal-->
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!-- Queremos que carge Login-->
        <activity android:name=".LoginActivity" />

        <!-- Queremos que carge sign up-->
        <activity android:name=".SignUpActivity" />

        <!-- Queremos que carge sign up usuario-->
        <activity android:name=".SignUpUsuarioActivity" />

        <!-- Queremos que carge sign up doctor-->
        <activity android:name=".SignUpDoctorActivity" />

        <!-- Queremos que carge sign up farmacia-->
        <activity android:name=".SignUpFarmaciaActivity" />

        <!-- Queremos que carge home usuario-->
        <activity android:name=".HomeUserActivity" />

        <!-- Queremos que carge perfil usuario-->
        <activity android:name=".PerfilUserActivity" />


    </application>

</manifest>